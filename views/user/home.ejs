    <script>
        function addToCart(event) {            
        
            const productId = event.currentTarget.getAttribute('data-product-id');
            const productq = event.currentTarget.getAttribute('data-quantity');
            
            if(productq < 1){
                showSnackbar('Out Of Stock')
                return;
            }

            const quantity = 1; // Default quantity, adjust as needed
            fetch('/addToCartFromHome', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({ id: productId, quantity: quantity })
            })
            .then(response => response.json())
            .then(result => {
                if(result.redirectUrl) {
                   window.location.href=result.redirectUrl
                } else if (result.success){
                    showSnackbar(result.message)
                }else{

                    showSnackbar(result.message)
                }
                    // Optionally, update the UI to show the product was added to the cart
                
            })
            .catch(error => console.error('Error:', error));
        }


        function addToWishlist(event, productId){            
            fetch('/addToWishlist',{
                method:'POST',
                headers:{
                    'content-type':'application/json'
                },
                body:JSON.stringify({productId:productId})
            }).then(response => response.json())
              .then(data=>{
                if(data.success){
                    showSnackbar('added to wish list')
                }else{
                    showSnackbar('eroor adding wishlist')
                }
              }).catch(error=>{
                console.log(error); 
              })
        }

        function showSnackbar(message) {
    var snackbar = document.getElementById("snackbar");
    snackbar.textContent = message;
    snackbar.className = "show";

    // Hide the snackbar after 3 seconds (3000 milliseconds)
    setTimeout(function() {
        snackbar.className = snackbar.className.replace("show", "");
    }, 3000);
}

