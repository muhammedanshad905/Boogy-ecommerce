        <script>
            function addToCart(productId) {
                const id = productId;                
                const quantity = 1;
        
                fetch(`/addToCart`, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'Application/json'
                    },
                    body: JSON.stringify({ quantity: quantity, id: id })
                })
                .then(response => response.json())
                .then(data => {
                    if (data.success) {
                        Swal.fire({
                            title: 'Success!',
                            text: data.message,
                            icon: 'success',
                            confirmButtonText: 'done'
                        });
                    }else{
                                    Swal.fire({
                            title: 'Error!',
                            text: data.message,
                            icon: 'error',
                            confirmButtonText: 'Try Again'
                        });
                    }
                })
                .catch(error => {
                    console.log(error);
                    
                    Swal.fire({
                        title: 'Error!',
                        text: 'faild to add cart',
                        icon: 'error',
                        confirmButtonText: 'Try Again'
                    });
                });
            }
        </script>
        
