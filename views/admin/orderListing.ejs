                                    <th class="product-name alt-font">Product</th>
                                    <th class="product-price text-center alt-font">Price</th>
                                    <th class="product-subtotal text-center alt-font">Payment Method</th>
                                    <th class="product-subtotal text-center alt-font">Status</th>
                                    <th class="product-subtotal text-center alt-font">Quantity</th>
                                    <th class="product-subtotal text-center alt-font">Details</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% order.forEach(orders => { %>
                                    <% orders.orderedItems.forEach(item => { %>
                                        <tr>

                                            <td class="product-thumbnail text-center">
                                                <a href="/admin/adminOrderDetails?orderId=<%=orders.id%>">
                                                    <img src="/assets/productImages/<%= item.productImage[0]%>" width="100" alt="<%= item.productName %>" title="<%= item.productName %>">
                                                </a>
                                            </td>
                                            <td class="product-name">
                                                <h6 class="mb-0"><a href="/admin/adminOrderDetails?orderId=<%=orders.id%>"><%= item.productName %></a></h6>
                                                <p class="mb-0 text-muted small">
                                                    Order ID: <%= orders._id %> <br>
                                                    Order Date: <%= new Date(orders.createdAt).toDateString() %>
                                                </p>
                                            </td>
                                            <td class="product-price text-center">
                                                <span class="amount fw-500">$<%= item.price.toFixed(2) %></span>
                                            </td>
                                            <td class="product-subtotal text-center">
                                                <%= orders.paymentMethod %>
                                            </td>
                                            <td class="product-subtotal text-center">
                                                <%= item.orderStatus %>
                                            </td>
                                            <td class="product-subtotal text-center">
                                                <%= item.quantity %>
                                            </td>
                                            <td class="product-subtotal text-center">
                                                <div class="dropdown">
                                                    <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton<%= item._id %>" data-bs-toggle="dropdown" aria-expanded="false">
                                                        Actions
                                                    </button>
                                                    <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton<%= item._id %>">
                                                        <% if(item.orderStatus === 'Pending' || item.orderStatus === 'success' ) {%>
                                                            <li><button onclick='updateStatus("<%= orders._id %>","<%= item._id %>", "Order Shipped")' class="dropdown-item" >Shipped</button></li>
                                                            <li><button onclick='updateStatus("<%= orders._id %>","<%= item._id %>", "Delivered")' class="dropdown-item">Delivered</button></li>
                                                        <% }else if(item.orderStatus === 'Delivered') {%>
                                                        <% }else if(item.orderStatus === 'Shipped') {%>
                                                            <li><button onclick='updateStatus("<%= orders._id %>","<%= item._id %>", "Delivered")' class="dropdown-item">Delivered</button></li>
                                                        <% }else if(item.orderStatus === 'Returned') {%>
                                                        <% }else if(item.orderStatus === 'Canceled') {%>
                                                        <% } %>
                                                        
                                                    </ul>
                                                </div>
                                            </td>
                                        </tr>
                                    <% }) %>
                                <% }) %>
                            </tbody>
