        <script>
            document.addEventListener('DOMContentLoaded', async () => {
                try {
                    const response = await fetch('/admin/topProducts'); // Assuming your route is '/top-products'
                    const topProducts = await response.json();
        
                    const tableBody = document.querySelector('tbody'); // Assuming there's only one tbody
        
                    topProducts.forEach(product => {
                        const row = document.createElement('tr');
        
                        row.innerHTML = `
                            <td class="border-bottom prod-img"><img src="/assets/productImages/${product.productImage[0]}" alt="product" width="50" /></td>
                            <td class="border-bottom prod-tlt">${product.productName}</td>
                            <td class="border-bottom text-center fw-500"><span>${product.totalQuantity}</span></td>
                        `;
        
                        tableBody.appendChild(row);
                    });
                } catch (error) {
                    console.error('Error loading top products:', error);
                }
            });
        </script>
        
        <script>
            document.addEventListener('DOMContentLoaded', function () {
    const timePeriodSelect = document.getElementById('timePeriodSelect');

    timePeriodSelect.addEventListener('change', fetchOrderStatusCounts);

    // Initial call to fetch and update the chart
    fetchOrderStatusCounts();

    function fetchOrderStatusCounts() {
        const selectedPeriod = timePeriodSelect.value;

        fetch(`/admin/dashStatus?period=${selectedPeriod}`)
            .then(response => response.json())
            .then(data => updateCharts(data.status, data.category, data.perDay))
            .catch(error => console.error('Error:', error));
    }

    let lineChart; // Declare variable to store the chart instance

    function updateCharts(orderStatusData, categoryData, perDay) {

        // Extract dates and counts from perDay data
        const dates = perDay.map(item => item._id); // Dates for x-axis labels
        const counts = perDay.map(item => item.count); // Counts for y-axis data

        const barColors = ["#4e73df", "#1cc88a", "#36b9cc", "#f6c23e", "#e74a3b", "#858796"];

        // Destroy the existing chart if it exists
        if (lineChart) {
            lineChart.destroy();
        }

        // Create or update the line chart for order counts per day
        lineChart = new Chart("myChart", {
            type: "line",
            data: {
                labels: dates, // Dates for x-axis labels
                datasets: [{
                    label: 'Order Count',
                    data: counts, // Corresponding counts for each date
                    borderColor: "#4e73df", // Line color
                    backgroundColor: "rgba(78, 115, 223, 0.1)", // Fill under the line
                    fill: true,
                    tension: 0.1 // Smooth the line
                }]
            },
            options: {
                responsive: true,
                plugins: {
                    legend: {
                        display: true // Show the legend
                    }
                },
                scales: {
                    y: {
                        beginAtZero: true, // Ensure y-axis starts at 0
                        title: {
                            display: true,
                            text: 'Order Count'
                        }
                    },
                    x: {
                        title: {
                            display: true,
                            text: 'Date'
                        }
                    }
                }
            }
        });
    }
});

